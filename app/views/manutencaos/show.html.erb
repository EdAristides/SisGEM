<div class="col-md-9 col-md-offset-2 col-sm-9 col-sm-offset-2 col-xs-12 conteudoInf">
  <p id="notice"><%= notice %></p>

  <p>
    <strong>Numordem:</strong>
    <%= @manutencao.numOrdem %>
  </p>

  <p>
    <strong>Patrimônio:</strong>
    <%= @manutencao.equipamento_id %>
  </p>

  <p>
    <strong>Descricao:</strong>
    <%= @manutencao.descricao %>
  </p>

  <p>
    <strong>Dataentrada:</strong>
    <%= @manutencao.dataEntrada %>
  </p>

<!--   <p>
    <strong>Datasaida:</strong>
    <%= @manutencao.dataSaida %>
  </p> -->

  <p>
    <strong>Status:</strong>
    <%= @manutencao.status %>
  </p>

  <div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
      <%= link_to 'Editar', edit_manutencao_path(@manutencao), class:"btn btn-primary"%>
      <%= link_to 'Voltar', manutencaos_path, class:"btn btn-default"%>
    </div>
  </div>

  <h3>Comentários</h3>
  <% @comments.each do |comment| %>
    <div class="well">
      <%= comment.user.name %> - <%= comment.created_at %>
      <p>
          <%= comment.body %>
      </div>
  <% end %>
  <%= paginate @comments, window: 2 %>

  <%= render partial: "comments/form", locals: {commentable: @manutencao} %>
  <!-- <%= render partial: "comments/comments", locals: {commentable: @manutencao} %>-->
</div>